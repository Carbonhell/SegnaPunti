<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">

	<display-name>SegnaPuntiServlet</display-name>

    <servlet>
        <servlet-name>SegnaPuntiServlet</servlet-name>
        <servlet-class>main.com.amazon.ask.colorpicker.SegnaPuntiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SegnaPuntiServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>

</web-app>